<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">special_lincomb_v([1],[[1,0]],2);</Text-field>
</Input>
<Output>
<Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display="">NyQiIiIiIiE=</Equation></Text-field>
</Output>
</Group>
