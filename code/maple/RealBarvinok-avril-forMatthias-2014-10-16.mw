<Text-field prompt="&gt; " style="Maple Input" layout="Normal">regular:=proc(t,alpha,n) local ok,p,i,pp,v,j,s,deg,newP,P,PP,out:
ok:=0;
P:=1;
for i from 1 to nops(alpha) do
P:=P*alpha[i];
od;

v:=[seq(t^i,i=0..n-1)];
for j from 1 to n
do P:=subs(x[j]=t^(j-1),P);
od:
deg:=degree(P);
s:=1; while ok=0 and s&lt;=deg+1 do
newP:=subs(t=s,P); ##print(newP);
if newP&lt;&gt;0 
then out:=[v,s,subs(t=s,v)];
ok:=1;
else s:=s+1;
fi;
od:
out[3];
end:</Text-field>
