t:=1; ok:=0;
 while t&lt;=10 do 
 w:=cone_random_vector(10*t,sigma);
f:=1; 
while f&lt;=nops(K) do
MM:=Matrix([seq(LL[K[f][i]],i=1..k-1),w]);
if Determinant(MM)&lt;&gt;0 
then 
f:=f+1;
ok:=ok:
 ##print('ok,f',ok);
 else f:=nops(K)+1;
 ok:=ok+1;
 fi:od;
if ok=0 then t:=11;
else
t:=t+1;ok:=0;
